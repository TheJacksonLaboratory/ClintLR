<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox minHeight="500.0" minWidth="1200.0" prefHeight="500.0" prefWidth="1200.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.monarchinitiative.l2ci.gui.controller.ResourcesController">
    <children>
        <Label contentDisplay="CENTER" minWidth="100.0" text="Initialize L4CI resources:" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Label>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="230.0" />
                <ColumnConstraints halignment="LEFT" hgrow="NEVER" prefWidth="60.0" />
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" />
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="10.0" />
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="10.0" />
                <ColumnConstraints halignment="CENTER" hgrow="NEVER" minWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="60.0" />
                <RowConstraints minHeight="10.0" />
                <RowConstraints minHeight="10.0" prefHeight="60.0" />
            </rowConstraints>
            <children>

                <Label text="HPO json" wrapText="true" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="hpJsonLabel" GridPane.columnIndex="2" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#setHPOFileButtonAction" text="Set path" GridPane.columnIndex="3" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#downloadHPOFileButtonAction" text="Download" GridPane.columnIndex="4" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <ProgressIndicator fx:id="hpoProgressIndicator" progress="0.0" GridPane.columnIndex="5" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </ProgressIndicator>
                <Label text="HPO Annotation File" wrapText="true" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="hpoaLabel" GridPane.columnIndex="2" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#setHPOAFileButtonAction" text="Set path" GridPane.columnIndex="3" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" fx:id="downloadHPOAButton" onAction="#downloadHPOAFileButtonAction" text="Download" GridPane.columnIndex="4" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <ProgressIndicator fx:id="hpoaProgressIndicator" progress="0.0" GridPane.columnIndex="5" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </ProgressIndicator>
                <Label text="Mondo json" wrapText="true" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="mondoLabel" GridPane.columnIndex="2" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#setMondoFileButtonAction" text="Set path" GridPane.columnIndex="3" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#downloadMondoFileButtonAction" text="Download" GridPane.columnIndex="4" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <ProgressIndicator fx:id="mondoProgressIndicator" progress="0.0" GridPane.columnIndex="5" GridPane.rowIndex="6">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </ProgressIndicator>
                <Label text="LIRICAL data directory" wrapText="true" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="liricalDataDirLabel" GridPane.columnIndex="2" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#setLiricalDataDirButtonAction" text="Set path" GridPane.columnIndex="3" GridPane.rowIndex="7">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
                <Label text="Exomiser variant file" wrapText="true" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="exomiserFileLabel" GridPane.columnIndex="2" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Label>
                <Button minHeight="30.0" minWidth="90.0" mnemonicParsing="false" onAction="#setExomiserVariantFileButtonAction" text="Set path" GridPane.columnIndex="3" GridPane.rowIndex="8">
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </GridPane.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </GridPane>
    </children>
    <effect>
        <DropShadow />
    </effect>
</VBox>
